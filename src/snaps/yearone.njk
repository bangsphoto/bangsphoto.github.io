---
layout: layout.njk
title: year one | bangsphoto
---
<div uk-lightbox="animation: slide">
    <div class="uk-grid-small uk-child-width-1-2@s" uk-grid>
        {% for image in yearone.images.slice(0,2) %}
        <a href="{{ image.path }}" data-caption="{{ image.caption }}">
            <img src="{{ image.path }}"
                alt="{{ image.caption }}"
                width="{{ image.width }}"
                height="{{ image.height }}"
                loading="eager" />
        </a>
        {% endfor %}
    </div>
    <div class="uk-grid-small uk-child-width-1-2@s uk-child-width-1-4@m" uk-grid>
        {% for image in yearone.images.slice(2) %}
        <a href="{{ image.path }}" data-caption="{{ image.caption }}">
            <img src="{{ image.path }}"
                alt="{{ image.caption }}"
                width="{{ image.width }}"
                height="{{ image.height }}"
                loading="{{"eager" if loop.index <= 4 else "lazy"}}" />
        </a>
        {% endfor %}
    </div>
</div>
{% include "totop.njk" %}
